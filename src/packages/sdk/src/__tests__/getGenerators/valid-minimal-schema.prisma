datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator gen {
  provider      = "predefined-generator"
  binaryTargets = ["darwin"]
}

generator gen_env {
  provider      = "predefined-generator"
  binaryTargets = env("binaryTargets_ENV_VAR_TEST")
}

model User {
  id   Int    @id
  name String
}
